{
    // Enforcing options
    // When set to true, these options will make JSHint produce more warnings about your code.
    "bitwise"       : true,
    "camelcase"     : false,
    "curly"         : true,
    "eqeqeq"        : true,
    "es3"           : false,
    "forin"         : true,
    "freeze"        : true,
    "immed"         : true,
    "indent"        : 4,
    "latedef"       : true,
    "newcap"        : true,
    "noarg"         : true,
    "noempty"       : true,
    "nonbsp"        : true,
    "nonew"         : false,
    "plusplus"      : false,
    "quotmark"      : false,
    "undef"         : true,
    "unused"        : true,
    "strict"        : true,
    "trailing"      : true,
    "maxstatements" : 25,
    "maxcomplexity" : 8,
    "maxlen"        : 160,

    // Relaxing options
    // When set to true, these options will make JSHint produce less warnings about your code.
    "asi"           : false,
    "boss"          : true,
    "debug"         : false,
    "eqnull"        : false,
    "esnext"        : false,
    "evil"          : false,
    "expr"          : false,
    "funcscope"     : false,
    "gcl"           : false,
    "globalstrict"  : false,
    "iterator"      : false,

    "lastsemic"     : false,
    "laxbreak"      : false,
    "laxcomma"      : false,
    "loopfunc"      : false,
    "maxerr"        : 50,
    "moz"           : false,
    "multistr"      : false,
    "notypeof"      : false,
    "proto"         : false,
    "scripturl"     : false,
    "smarttabs"     : false,
    "shadow"        : false,
    "sub"           : false,
    "supernew"      : false,
    "validthis"     : false,
    "noyield"       : false,

    // Environments
    // These options let JSHint know about some pre-defined global variables.
    "browser"       : true,
    "couch"         : false,
    "devel"         : false,
    "dojo"          : false,
    "jquery"        : true,
    "mootools"      : false,
    "node"          : false,
    "nonstandard"   : false,
    "phantom"       : false,
    "prototypejs"   : false,
    "rhino"         : false,
    "worker"        : false,
    "wsh"           : false,
    "yui"           : false
}
